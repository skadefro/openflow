{
  "name": "@openiap/core",
  "version": "1.5.10.7",
  "description": "Simple  wrapper around NodeRed, RabbitMQ and MongoDB to support a more scaleable NodeRed implementation.\r Also the \"backend\" for [OpenRPA](https://github.com/skadefro/OpenRPA)",
  "main": "index.js",
  "scripts": {
    "test": "clear && _mocha 'src/**/*.test.ts'",
    "coverage": "nyc ./node_modules/.bin/_mocha 'test/**/*.test.ts'",
    "oldtest": "ts-mocha --paths -p tsconfig.json **/*.spec.ts",
    "sass": "gulp sass",
    "watch": "gulp watch",
    "compose": "gulp compose",
    "latest": "gulp latest",
    "build": "tsc --build tsconfig.json",
    "web1": "npm uninstall @openiap/core-web && npm install @openiap/core-web@1.0.0",
    "web2": "npm uninstall @openiap/core-web && npm install @openiap/core-web@2.0.0",
    "web3": "npm uninstall @openiap/core-web && npm install @openiap/core-web@3.0.8",
    "weblocal": "npm uninstall @openiap/core-web && npm install ../core-web",
    "updateapilocal": "npm uninstall @openiap/nodeapi && npm i ../nodeapi",
    "updateapidev": "npm uninstall @openiap/nodeapi && npm i openiap/nodeapi",
    "updateapi": "npm uninstall @openiap/nodeapi && npm i @openiap/nodeapi",
    "dockertest": "npm run compose && docker run -t -i --entrypoint=/bin/bash openiap/openflow:edge -s",
    "dockerbuild": "docker compose up --build -d",
    "spec": "tsoa spec",
    "routes": "tsoa routes",
    "generate": "tsoa spec && tsoa routes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openiap/core.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "OpenIAP ApS / Allan Zimmermann",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://community.openiap.io/"
  },
  "type": "module",
  "homepage": "https://github.com/openiap/core",
  "funding": "https://github.com/sponsors/skadefro",
